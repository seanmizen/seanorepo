{
  "name": "seanmizen.com",
  "type": "module",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "rsbuild dev --open",
    "start:noopen": "yarn exec rsbuild dev --port 4000",
    "start:docker": "docker compose --profile dev up --build",
    "prod:docker": "BUILD_TARGET=prod docker compose --profile prod up --build --detach",
    "build": "rsbuild build",
    "down": "docker compose --profile dev --profile prod down"
  },
  "dependencies": {
    "number-to-words": "^1.2.4",
    "react": "^19.1.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "stats.js": "^0.17.0",
    "three": "^0.176.0",
    "three-stdlib": "^2.35.16"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.3.5",
    "@rsbuild/plugin-react": "^1.2.0",
    "rsbuild-plugin-glsl": "^1.0.1"
  }
}
