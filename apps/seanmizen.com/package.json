{
  "name": "seanmizen.com",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "yarn rsbuild dev --open",
    "start:noopen": "yarn rsbuild dev --port 4000",
    "start:docker": "docker compose --profile dev up --build",
    "prod:docker": "BUILD_TARGET=prod docker compose --profile prod up --build --detach",
    "build": "yarn && yarn rsbuild build",
    "down": "docker compose --profile dev --profile prod down"
  },
  "dependencies": {
    "number-to-words": "^1.2.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "stats.js": "^0.17.0",
    "three": "^0.176.0",
    "three-stdlib": "^2.36.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.13",
    "@rsbuild/plugin-react": "^1.4.2",
    "@types/number-to-words": "^1.2.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "rsbuild-plugin-glsl": "^1.0.1",
    "typescript": "^5.9.3"
  }
}
